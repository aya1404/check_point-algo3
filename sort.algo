ALGORITHM insertion_sort
VAR
    arr  : ARRAY_OF INTEGER[n];
    i,j , temp : INTEGER
BEGIN
    write("enter a number")
    read(arr[i])
    FOR i FROM 0 TO arr.lenght-1 STEP  1  DO
        j = i+1
        WHILE (arr[j] < arr[j-1]) DO
            temp := arr[j]
            arr[j] := arr[j-1]
            arr [j-1] := temp
        END_WHILE
    END_FOR END 
